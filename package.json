{
  "name": "editor-backend-express",
  "version": "1.0.0",
  "author": "Daniel Andersson",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "development": "NODE_ENV='development' nodemon server.js",
    "dev": "NODE_ENV='development' nodemon server.js",
    "dev-test": "NODE_ENV='test' && jest --watch",
    "production": "NODE_ENV='production' node server.js",
    "test": "NODE_ENV='test' && jest --silent",
    "start": "NODE_ENV='production' node server.js"
  },
  "keywords": [
    "api",
    "texteditor",
    "express",
    "mongodb",
    "babel",
    "webpack"
  ],
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongodb": "^4.1.2",
    "mongoose": "^5.13.9",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^7.0.2",
    "jest": "^27.2.1",
    "nodemon": "^2.0.12",
    "prettier": "^1.15.2",
    "superagent": "^6.1.0",
    "supertest": "^6.1.6"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "node_modules/*"
    ],
    "verbose": true,
    "execMap": {
      "js": "node"
    },
    "runOnChangeOnly": false,
    "watch": [
      "src/**/*.js",
      "src/**/*.json"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json"
  }
}
